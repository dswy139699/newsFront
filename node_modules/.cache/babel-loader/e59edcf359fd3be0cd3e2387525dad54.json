AA,CAAA,GAAA,CAAA,oBAAA,GAAA,+CAAA,GAAA,IAAA,CAAA,QAAA,GAAA,YAAA,GAAA,IAAA,CAAA,QAAA;AACA,KAbA;AAcA,IAAA,MAdA,oBAcA;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,WAAA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,KAjBA;AAkBA,IAAA,QAlBA,sBAkBA;AACA,UAAA,MAAA,GAAA,KAAA,KAAA,CAAA,WAAA,CAAA,YAAA,EAAA;;AACA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,gBAAA,CAAA,KAAA,IAAA,EAAA;;AACA,YAAA,KAAA,IAAA,KAAA,CAAA,EAAA;AACA,eAAA,QAAA,CAAA,MAAA;AACA,SAFA,MAEA,IAAA,KAAA,IAAA,KAAA,CAAA,EAAA;AACA,eAAA,SAAA,CAAA,MAAA;AACA,SAFA,MAEA,IAAA,KAAA,IAAA,KAAA,CAAA,EAAA;AACA,eAAA,YAAA,CAAA,MAAA;AACA,SAFA,MAEA,IAAA,KAAA,IAAA,KAAA,CAAA,EAAA;AACA,eAAA,UAAA,CAAA,MAAA;AACA;;AACA,aAAA,OAAA,GAAA,KAAA;AACA;AACA;AAjCA;AApBA,CAAA","sourcesContent":["<template>\r\n  <page-header-wrapper>\r\n    <template v-slot:content>\r\n      <div class=\"page-header-content\">\r\n      </div>\r\n    </template>\r\n    <div class=\"content\">\r\n      <div>\r\n        <a-button type=\"primary\" style=\"margin-right:10px\" @click=\"visible = true,type=0\">导出未征订学生</a-button>\r\n<!--        <a-button type=\"primary\" style=\"margin-right:10px\" @click=\"visible = true,type=1\">导出所有学生付款信息</a-button>-->\r\n        <a-button type=\"primary\" style=\"margin-right:10px\" @click=\"visible = true,type=2\">导出学生退款信息</a-button>\r\n        <a-button type=\"primary\" @click=\"visible = true,type=3\">导出未补款学生</a-button>\r\n      </div>\r\n      <div>\r\n        <div class=\"modal\">\r\n          <a-modal\r\n            title=\"信息确认\"\r\n            :visible=\"visible\"\r\n            @cancel=\"cancel\"\r\n            @ok=\"handelOk\"\r\n          >\r\n            <div class=\"increase\">\r\n              <GlobalForm ref=\"increaseDom\" :dataSource=\"getFormData\" id=\"increaseDom\">\r\n              </GlobalForm>\r\n            </div>\r\n          </a-modal>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </page-header-wrapper>\r\n</template>\r\n\r\n<script>\r\n  // import { services } from './service'\r\n  import GlobalForm from '@/components/GlobalForm'\r\n  import { formData } from './formData'\r\n  export default {\r\n    name: 'Index',\r\n    components: {\r\n      GlobalForm\r\n    },\r\n    data () {\r\n      return {\r\n        // schoolId: '',\r\n        type: 0,\r\n        visible: false\r\n      }\r\n    },\r\n    computed: {\r\n      getFormData () {\r\n        return formData.getFormData(this)\r\n      }\r\n    },\r\n    mounted () {\r\n      // this.schoolId = this.$store.getters.schoolNameList[0] ? this.$store.getters.schoolNameList[0].value : ''\r\n    },\r\n    methods: {\r\n      exportNo (data) { // 导出未征订学生\r\n        console.log(data)\r\n        window.open(process.env.VUE_APP_API_BASE_URL + '/export/downloadIsNotSubscriptionsStudent?schoolId=' + data.schoolId + '&password=' + data.loginPwd)\r\n      },\r\n      exportAll (data) { // 导出所有学生付款信息\r\n        window.open(process.env.VUE_APP_API_BASE_URL + '/export/downloadPaymentStudent?schoolId=' + data.schoolId + '&password=' + data.loginPwd)\r\n      },\r\n      exportReturn (data) { // 导出学生退款信息\r\n        window.open(process.env.VUE_APP_API_BASE_URL + '/export/downloadRefundStudent?schoolId=' + data.schoolId + '&password=' + data.loginPwd)\r\n      },\r\n      exportNoBu (data) { // 导出未补款学生\r\n        window.open(process.env.VUE_APP_API_BASE_URL + '/export/downloadFillingMoneyStudent?schoolId=' + data.schoolId + '&password=' + data.loginPwd)\r\n      },\r\n      cancel () {\r\n        this.$refs.increaseDom.resetFields()\r\n        this.visible = false\r\n      },\r\n      handelOk () {\r\n        const params = this.$refs.increaseDom.handleSubmit()\r\n        if (params) {\r\n          params.schoolId = this.$store.getters.selectSchoolInfo.value || ''\r\n          if (this.type === 0) {\r\n            this.exportNo(params)\r\n          } else if (this.type === 1) {\r\n            this.exportAll(params)\r\n          } else if (this.type === 2) {\r\n            this.exportReturn(params)\r\n          } else if (this.type === 3) {\r\n            this.exportNoBu(params)\r\n          }\r\n          this.visible = false\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.content{\r\n  background: #fff;\r\n  padding:20px;\r\n  border-radius:2px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/solicitManage/export"}]}