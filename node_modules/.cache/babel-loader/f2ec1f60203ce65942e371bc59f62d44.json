A;AAAA;AAFA,GAAA,CADA,CAxCA;AA8CA,EAAA,OA9CA,qBA8CA;AACA,QAAA,MAAA,GAAA,KAAA,QAAA,CAAA,IAAA,CAAA,UAAA,IAAA;AAAA,aAAA,IAAA,CAAA,IAAA,KAAA,GAAA;AAAA,KAAA,CAAA;AACA,SAAA,KAAA,GAAA,MAAA,IAAA,MAAA,CAAA,QAAA,IAAA,EAAA,CAFA,CAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,GAxDA;AAyDA,EAAA,OAzDA,qBAyDA;AAAA;;AACA;AACA;AACA,QAAA,SAAA,GAAA,SAAA,CAAA,SAAA;;AACA,QAAA,SAAA,CAAA,OAAA,CAAA,MAAA,IAAA,CAAA,CAAA,EAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,CAAA,KAAA,CAAA,SAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,CAAA,KAAA,CAAA,SAAA;AACA,SAFA,EAEA,EAFA,CAAA;AAGA,OALA;AAMA,KAXA,CAaA;AACA;;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,KAAA,YAAA,IAAA,OAAA,CAAA,GAAA,CAAA,eAAA,KAAA,MAAA,EAAA,CACA;AACA;AACA,GA3EA;AA4EA,EAAA,OAAA,kCACA,UAAA,CAAA,CAAA,WAAA,EAAA,UAAA,CAAA,CADA;AAEA;AACA,IAAA,gBAHA,4BAGA,GAHA,EAGA;AACA,WAAA,KAAA,GAAA,GAAA;;AACA,UAAA,KAAA,QAAA,IAAA,CAAA,GAAA,CAAA,WAAA,CAAA,EAAA;AACA,aAAA,QAAA,GAAA,KAAA;AACA;AACA;;AACA,UAAA,CAAA,KAAA,QAAA,IAAA,GAAA,CAAA,WAAA,CAAA,EAAA;AACA,aAAA,QAAA,GAAA,IAAA;AACA,aAAA,SAAA,GAAA,KAAA;AACA,aAAA,QAAA,CAAA,YAAA,GAAA,OAAA;AACA,aAAA,QAAA,CAAA,WAAA,GAAA,KAAA;AACA;AACA,KAfA;AAgBA,IAAA,cAhBA,0BAgBA,GAhBA,EAgBA;AACA,WAAA,SAAA,GAAA,GAAA;AACA,KAlBA;AAmBA,IAAA,UAnBA,wBAmBA;AAAA;AACA;AACA;AArBA;AA5EA,CAAA","sourcesContent":["<template>\n  <pro-layout\n    :title=\"title\"\n    :menus=\"menus\"\n    :collapsed=\"collapsed\"\n    :handleMediaQuery=\"handleMediaQuery\"\n    :handleCollapse=\"handleCollapse\"\n    :logo=\"logoRender\"\n    v-bind=\"settings\"\n  >\n    <template v-slot:rightContentRender>\n      <right-content :top-menu=\"settings.layout === 'topmenu'\" :is-mobile=\"isMobile\" :theme=\"settings.theme\" />\n    </template>\n    <template v-slot:footerRender>\n      <div></div>\n    </template>\n    <router-view />\n  </pro-layout>\n</template>\n\n<script>\nimport { SettingDrawer } from '@ant-design-vue/pro-layout'\n// import { i18nRender } from '@/locales'\nimport { mapState, mapActions } from 'vuex'\n// import { CONTENT_WIDTH_TYPE } from '@/store/mutation-types'\n\nimport defaultSettings from '@/config/defaultSettings'\nimport RightContent from '@/components/GlobalHeader/RightContent'\nimport LogoSvg from '../assets/logo.svg?inline'\n\nexport default {\n  name: 'BasicLayout',\n  components: {\n    SettingDrawer,\n    RightContent\n  },\n  data () {\n    return {\n      // preview.pro.antdv.com only use.\n      isProPreviewSite: process.env.VUE_APP_PREVIEW === 'true' && process.env.NODE_ENV !== 'development',\n      // end\n\n      // base\n      menus: [],\n      // 侧栏收起状态\n      collapsed: false,\n      title: defaultSettings.title,\n      settings: {\n        // 布局类型\n        layout: defaultSettings.layout, // 'sidemenu', 'topmenu'\n        // CONTENT_WIDTH_TYPE\n        contentWidth: defaultSettings.layout === 'sidemenu' ? 'Fluid' : defaultSettings.contentWidth,\n        // 主题 'dark' | 'light'\n        theme: defaultSettings.navTheme,\n        // 主色调\n        primaryColor: defaultSettings.primaryColor,\n        fixedHeader: defaultSettings.fixedHeader,\n        fixSiderbar: defaultSettings.fixSiderbar,\n        colorWeak: defaultSettings.colorWeak,\n\n        hideHintAlert: false,\n        hideCopyButton: false\n      },\n      // 媒体查询\n      query: {},\n\n      // 是否手机模式\n      isMobile: false\n    }\n  },\n  computed: {\n    ...mapState({\n      // 动态主路由\n      mainMenu: state => state.permission.addRouters\n    })\n  },\n  created () {\n    const routes = this.mainMenu.find(item => item.path === '/')\n    this.menus = (routes && routes.children) || []\n    // 处理侧栏收起状态\n    // this.$watch('collapsed', () => {\n    //   this.$store.commit(SIDEBAR_TYPE, this.collapsed)\n    // })\n    // this.$watch('isMobile', () => {\n    //   this.$store.commit(TOGGLE_MOBILE_TYPE, this.isMobile)\n    // })\n  },\n  mounted () {\n    // this.GetSchool().then()\n    // this.GetClass().then()\n    const userAgent = navigator.userAgent\n    if (userAgent.indexOf('Edge') > -1) {\n      this.$nextTick(() => {\n        this.collapsed = !this.collapsed\n        setTimeout(() => {\n          this.collapsed = !this.collapsed\n        }, 16)\n      })\n    }\n\n    // first update color\n    // TIPS: THEME COLOR HANDLER!! PLEASE CHECK THAT!!\n    if (process.env.NODE_ENV !== 'production' || process.env.VUE_APP_PREVIEW === 'true') {\n      // updateTheme(this.settings.primaryColor)\n    }\n  },\n  methods: {\n    ...mapActions(['GetSchool', 'GetClass']),\n    // i18nRender,\n    handleMediaQuery (val) {\n      this.query = val\n      if (this.isMobile && !val['screen-xs']) {\n        this.isMobile = false\n        return\n      }\n      if (!this.isMobile && val['screen-xs']) {\n        this.isMobile = true\n        this.collapsed = false\n        this.settings.contentWidth = 'Fluid'\n        this.settings.fixSiderbar = false\n      }\n    },\n    handleCollapse (val) {\n      this.collapsed = val\n    },\n    logoRender () {\n      return <LogoSvg />\n    }\n  }\n}\n</script>\n\n<style lang=\"less\">\n@import \"./BasicLayout.less\";\n</style>\n"],"sourceRoot":"src/layouts"}]}