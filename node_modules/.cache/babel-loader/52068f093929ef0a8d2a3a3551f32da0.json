r\n        },\r\n        updateStudent (data) {\r\n            updateStudent(data).then(response => {\r\n              this.$router.push({ path: '/book/student/unPayed' })\r\n            })\r\n        }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n  @import \"../style.less\";\r\n  .content{\r\n  background: #fff;\r\n  padding:60px 0;\r\n  border-radius:2px;\r\n      display: flex;\r\n      justify-content: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/studentView/studentInfo"}]}