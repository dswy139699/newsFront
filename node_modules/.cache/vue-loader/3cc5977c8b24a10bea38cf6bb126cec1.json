iv class=\"table\" style=\"margin-top:10px;\">\r\n            <a-table\r\n              :columns=\"columns\"\r\n              :data-source=\"data\"\r\n              :pagination=\"false\"\r\n              :rowKey=\"record=>record.teachingMaterialName\"\r\n            >\r\n              <template slot=\"footer\" slot-scope=\"\" v-if=\"data&&data.length>0\">\r\n                <div style=\"text-align: center\">总书册数：{{ formData.bookCount }}册    原价：{{ formData.totalPrice }}元    折后总价：{{ formData.discountPrice }}元</div>\r\n              </template>\r\n            </a-table>\r\n          </div>\r\n        </a-card>\r\n        <div style=\"text-align: right\">\r\n          <a-button type=\"primary\" @click=\"print\">\r\n            打印学生征订信息\r\n          </a-button>\r\n          <a-button type=\"primary\" style=\"margin-left:10px;margin-right:24px;\" @click=\"()=>{}\">\r\n            导出学生征订信息\r\n          </a-button>\r\n        </div>\r\n      </div>\r\n    </a-spin>\r\n  </page-header-wrapper>\r\n</template>\r\n\r\n<script>\r\n  import GlobalForm from '@/components/GlobalForm'\r\n  import { formData } from './formData'\r\n  import { services } from '../../service'\r\n\r\n  const columns = [\r\n    {\r\n      title: '课程名称',\r\n      dataIndex: 'name',\r\n      key: 'name'\r\n    },\r\n    {\r\n      title: '课程号',\r\n      dataIndex: 'num',\r\n      key: 'num'\r\n    },\r\n    {\r\n      title: '教材名',\r\n      dataIndex: 'teachingMaterialName',\r\n      key: 'teachingMaterialName'\r\n    },\r\n    {\r\n      title: '出版社名称',\r\n      dataIndex: 'press',\r\n      key: 'press'\r\n    },\r\n    {\r\n      title: 'ISBN',\r\n      dataIndex: 'isbn',\r\n      key: 'isbn'\r\n    },\r\n    {\r\n      title: '定价',\r\n      dataIndex: 'price',\r\n      key: 'price'\r\n    },\r\n    {\r\n      title: '折扣',\r\n      dataIndex: 'discount',\r\n      key: 'discount'\r\n    }\r\n  ]\r\n  export default {\r\n    name: 'Index',\r\n    components: {\r\n      GlobalForm\r\n    },\r\n    data () {\r\n      return {\r\n        columns,\r\n        spinning: true,\r\n        formData: {},\r\n        data: []\r\n      }\r\n    },\r\n    computed: {\r\n      getFormData () {\r\n        return formData.getFormData(this, this.formData)\r\n      }\r\n    },\r\n    mounted () {\r\n      const params = this.$route.query || {}\r\n      this.getRecordById({ ...params, schoolId: this.$store.getters.selectSchoolInfo.value || '' })\r\n    },\r\n    methods: {\r\n      print () {\r\n        // window.print()\r\n        const params = this.$route.query || {}\r\n        this.$router.push({ path: '/book/solicitManage/printInfo', query: { id: params.id, isPayment: 0 } })\r\n      },\r\n      getRecordById (data) {\r\n        services.getRecordById({ ...data, isPayment: 0 }).then(response => {\r\n          this.formData = response || {}\r\n          this.data = response.subscriptionsDetailList || []\r\n          this.spinning = false\r\n        })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n  @import '../../../style.less';\r\n  .content{\r\n  background: #fff;\r\n  padding:20px;\r\n  border-radius:2px;\r\n  .modal {\r\n    /deep/ .ant-modal {\r\n      width: 900px;\r\n      min-width: 900px;\r\n    }\r\n    /deep/ .ant-alert {\r\n      padding: 5px 5px 5px 35px;\r\n      font-size: 12px;\r\n      .ant-alert-icon {\r\n        top: 8px;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}